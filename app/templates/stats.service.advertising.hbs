<div class="chart">  <div class="details">    <div class="field">      <label>Number of active users</label>       {{input type="text" value=users}}     </div>    <div class="field">      <label>Number of streams</label>       {{input type="text" value=stream}}     </div>    <div class="field">      <label>Hours of streams</label>       {{input type="text" value=streamlen}}     </div>    <div class="field">       <label>Royalties, USD</br>(excl. VAT)</label>       {{input type="text" value=royalty}}     </div>  </div>    <div class="chart-container">    <div class="axis-caption">      <label>Number of streams</label>    </div>    {{service-advertising-chart lineData=chart selectedInterval=interval action="didChartClick"}}  </div></div>