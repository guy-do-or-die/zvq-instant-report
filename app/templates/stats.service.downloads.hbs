<div class="chart">  <div class="details">    <div class="field">      <label>Number of active users</label>       {{input type="text" value=users}}     </div>    <div class="field">       <label>Number of downloads</label>       {{input type="text" value=load}}     </div>    <div class="field">       <label>Royalties, USD</br>(excl. VAT)</label>       {{input type="text" value=royalty}}     </div>  </div>    <div class="chart-container">    <div class="axis-caption">      <label>Number of downloads</label>    </div>    {{service-downloads-chart lineData=chart selectedInterval=interval action="didChartClick"}}  </div></div>