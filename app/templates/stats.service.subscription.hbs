<div class="chart">  <div class="details">    <div class="field">      <label>Number of subscribers</label>       {{input type="text" value=sub}}     </div>    <div class="field">      <label>Number of streams</label>       {{input type="text" value=stream}}     </div>    <div class="field">      <label>Hours of streams</label>       {{input type="text" value=streamlen}}     </div>    <div class="field">       <label>Number of downloads</label>       {{input type="text" value=load}}     </div>    <div class="field">       <label>Royalties</label>       {{input type="text" value=royalty}}     </div>  </div>    <div class="chart-container">    <div class="axis-caption">      <label>Number of subscribers</label>    </div>    {{service-subscription-chart lineData=chart selectedInterval=interval action="didChartClick"}}  </div></div>