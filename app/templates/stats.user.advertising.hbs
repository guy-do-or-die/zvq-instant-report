<div class="chart">  <div class="details">    <div class="field">      <label>Number of active users</label>       {{input type="text" value=activeUsers}}     </div>    <div class="field">      <label>Number of new users</label>       {{input type="text" value=newUsers}}     </div>    <div class="field">      <label>User engagement, %</label>       {{input type="text" value=engagement}}     </div>    <div class="field">       <label>Lifetime value</label>       {{input type="text" value=lifetime}}     </div>  </div>    <div class="chart-container">    <div class="axis-caption">      <label>Active users</label>    </div>    {{user-advertising-chart lineData=chart selectedInterval=interval action="didChartClick"}}  </div></div>